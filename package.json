{
  "name": "react_vite_ts_fsd",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "src",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite --config ./config/vite.config.ts",
    "dev:backend": "ts-node-dev ./server/server.ts",
    "build:backend": "tsc",
    "build:frontend": "vite build --config config/vite.config.ts",
    "start:backend": "ts-node ./server/server.ts",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "decor": " npm run format && npm run lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "@tailwindcss/vite": "^4.1.5",
    "@types/lodash.merge": "^4.6.9",
    "@types/react-icons": "^2.2.7",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "lodash.merge": "^4.6.2",
    "prettier": "^3.5.1",
    "react-icons": "^5.5.0",
    "react-lite-youtube-embed": "^2.5.1",
    "react-player": "^2.16.0",
    "tailwindcss": "^4.1.5"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite-plugin-sass-dts": "^1.3.31"
  }
}
